#creating an array
a=[]
n=int(input("Enter size of array"))
i=1
while(i<n+1):
	a.append(input("Enter element;"))
	i+=1
print(a)

#sorting the array created
def bubbleSort(a):
  n = len(a)
  for i in range(n-1):
    for j in range(0, n-i-1):
      if a[j]>a[j+1]:
        a[j],a[j+1]=a[j+1],a[j]
(bubbleSort(a)
print("Sorted Array is:")
print(a)
